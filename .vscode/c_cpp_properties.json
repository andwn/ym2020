{
    "configurations": [
        {
            "name": "MegaDrive",
            "includePath": [
                "${workspaceRoot}/inc",
                "${workspaceRoot}/res",
                "${HOME}/mars/m68k-elf/include"
            ],
            "defines": [],
            "intelliSenseMode": "linux-gcc-x86",
            "browse": {
                "path": [
                    "${workspaceRoot}/inc",
                    "${workspaceRoot}/res",
                    "${workspaceRoot}/src/*"
                ],
                "limitSymbolsToIncludedHeaders": true,
                "databaseFilename": ""
            },
            "cStandard": "c99",
            "cppStandard": "c++11",
            "compilerPath": "/usr/bin/gcc",
            "compilerArgs": [
                "-ffreestanding"
            ]
        }
    ],
    "version": 4
}